@model GetOrganizationsModel
@{
    ViewData["Title"] = "See organizations";
    ViewData["ActivePage"] = ManageFacilitiesPages.DownloadOrganizations;
}
<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        <ul class="nav nav-pills flex-column">
            @foreach (var organization in Model.OrganizationDtos)
            {
                <li>
                    @Html.ActionLink(organization.Name, "Organization", "Facilities", new {organizationId = organization.Id}, null)
                </li>
            }
        </ul>
    </div>
</div>