{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "File": {
      "Path": "C:/DagAir_Logs/DagAir_Sensor_State_History_logs.txt"
    }
  },
  "rabbitMq":
  {
    "hostName": "localhost",
    "virtualHost": "dagair",
    "protocol": "rabbitmq://",
    "connectionName": "sensor-state-history-node"
  },
  "DagAirInfluxConfiguration": {
    "token": "local_token1",
    "org": "local_org1",
    "bucketName": "local_bucket1",
    "url": "http://localhost:8086",
    "retention": 3600
  }
}
