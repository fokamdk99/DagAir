@model RoomModel
<h4>Room with Id @Model.AdminNodeRoomDto.RoomDto.Id</h4>
<div class="row">
    <div class="col-12">
        <hr/>
    </div>
</div>
<div class="row">
    <div class="col-6 vh-100 overflow-auto">
        <ul id="messagesList">
            @foreach (var room in Model.AdminNodeRoomDto.PastMeasurements.Measurements)
            {
                <li class="list-group-item">
                    Measurement from @room.Item1.Date . Temperature: @room.Item1.Temperature
                </li>
            }
        </ul>
    </div>
</div>
<script src="~/js/signalr/dist/browser/signalr.js"></script>
<script type="text/javascript" src="~/js/@Model.Environment"></script>
<script src="~/js/rooms/chat.js"></script>
<link rel="stylesheet" type="text/css" href="~/css/rooms/site.css">
<script>
SetUniqueRoomId('@Model.AdminNodeRoomDto.RoomDto.Id');
</script>