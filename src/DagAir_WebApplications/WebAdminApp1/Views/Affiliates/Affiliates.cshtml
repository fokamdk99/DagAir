@model AffiliatesModel
@{
    ViewData["Title"] = "See affiliates";
    ViewData["ActivePage"] = ManageFacilitiesPages.DownloadAffiliates;
}
<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        <ul class="list-group">
            @foreach (var affiliate in Model.AdminNodeAffiliateDtos)
            {
                <li class="list-group-item">
                    @Html.ActionLink(affiliate.AffiliateDto.Name, "Affiliate", "Affiliates", new {affiliateId = affiliate.AffiliateDto.Id}, null)
                </li>
                
            }
        </ul>
    </div>
</div>